$('#weixinCreate').submit(function() {
    jQuery.ajax({
    url:'/wxsq/account/weixin/create',
    data:$('#weixinCreate').serialize(),
    type:"POST",  
    success:function(data)
    {
        　		$("#container_iframe").html(data); 
    }
    });
    return false;
});

#if($!{wxAccount} && $!{wxAccount.id} !="")
	var  defwx = "$!{wxAccount.id}";
#else
	var  defwx = "";
#end 



function chWx(target){
		
	var $this = $(target),_clickTab = $this.attr('link'); 
	$.get(_clickTab,function (data ,status){
		if(data!=null){
			if(data.status>0){
				 $this.removeClass("btn-default");
				 $this.addClass("btn-success");
				 
				 if( defwx == $this.attr("id")){
				 	
				 }else{
				 	
				 	$("#useweixin").html("当前微信:"+data.wxAccount.weixinName);
				 	$this.html("正在使用");
				    var temp = $("#"+defwx);
				 	temp.removeClass("btn-success");
				 	temp.addClass("btn-default");
					temp.html("开启使用");				
					defwx =  $this.attr("id");		
				 }
				 
				 
				
				 alert(data.success);
 			}else{
 					
					alert(data.error);
			}
		
		}else{
		
			alert();
		}
	});

}

</SCRIPT>

